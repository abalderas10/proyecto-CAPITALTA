{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "root": "apps/web",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.capitalta.abdev.click/api/:path*"
    }
  ]
}
